<%@ Master Language="C#" AutoEventWireup="true" CodeFile="admin1.master.cs" Inherits="admin_admin" %>
<%@ Register Src="menu.ascx" TagName="menu" TagPrefix="uc1" %>
<%@ Import Namespace="BlogEngine.Core" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" style="height: 100%;">
<head id="Head1" runat="server">
    <title>BlogEngine.NET Admin</title>
    <link rel="stylesheet" href="style.css" type="text/css" />
    <link rel="shortcut icon" href="~/pics/blogengine.ico" type="image/x-icon" />
</head>
<body>
    <div class="page">
        <form id="form1" runat="server">

            <div id="StatusBox">
                <div id="AdminStatus" runat="server" style="display:none"></div>
            </div>

            <div class="banner">
                <div class="be-logo">
                    <span>V<%=BlogSettings.Instance.Version() %> Admin</span>
                </div>                                  
                <div class="banner-links">
                    Welcome <%= AdminProfile() == null ? Page.User.Identity.Name : AdminProfile().DisplayName %>
                    | <a href="<%=Utils.RelativeWebRoot %>admin/Users/Profile.aspx?id=<%=Page.User.Identity.Name %>"><%=Resources.labels.myProfile %></a>
                    | <a href="<%=Utils.RelativeWebRoot %>"><%=Resources.labels.goToFrontPage %></a>
                    | <asp:LoginStatus ID="LoginStatus1" runat="server" SkinID="adminLoginStatus"  />
                </div>                                   
            </div>
            
            <div style="padding: 0px 18px;">
                <div class="tabs" style="width:100%">
                    <uc1:menu ID="Menu1" runat="server" />
                </div>
                <div class="content-box"> 
                    <asp:ContentPlaceHolder ID="cphAdmin" runat="server" />                                                                                                 
                </div>                                              
            </div>
            <div id="footer">
                <form id="paypal" action="https://www.paypal.com/cgi-bin/webscr" target="_blank" method="post" style="float:left;margin-left:10px">
                <input type="hidden" name="cmd" value="_s-xclick" />
                <a href="http://dotnetblogengine.net">BlogEngine.NET <%=BlogSettings.Instance.Version() %></a> | <a href="http://www.paypal.com" onclick="document.getElementById('paypal').submit();return false;"><%=Resources.labels.makeDonation %></a>
                <img alt="" border="0" src="https://www.paypal.com/en_US/i/scr/pixel.gif" width="1" height="1" />
                <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----" />
                </form>                        
            </div>

        </form>    
    </div>
</body>
</html>